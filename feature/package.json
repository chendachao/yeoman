        "meta": {
          "ace.js": {
            "exports": "ace",
            "format": "global"
          },
          "*.js": {
            "deps": [
              "ace/ace"
            ],
            "format": "global"
          }
        }
      },
      "github:twbs/bootstrap@3.3.7": {
        "main": "js/bootstrap.js",
